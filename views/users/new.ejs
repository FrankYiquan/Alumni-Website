<div class="data-form">
    <form action="/users/create" method="POST">
      <br><br>
      <h2 class = "newHead">Create a new user:</h2>
      <label for="inputName">Name</label>
      <input
        type="text"
        name="name"
        id="inputName"
        placeholder="Name"
        autofocus
      />
      <label for="inputPassword">Password</label>
      <input
        type="password"
        name="password"
        id="inputPassword"
        placeholder="Password"
        required
      />
      <label for="inputEmail">Email address</label>
      <input
        type="email"
        name="email"
        id="inputEmail"
        placeholder="Email address"
        required
      />
      <label for="inputZipCode">Zip Code</label>
      <input
        type="text"
        name="zipCode"
        id="inputZipCode"
        pattern="\d*"
        placeholder="Zip Code"
        required
      />
      <label for="inputRole">Role</label>
      <input
        type="text"
        name="role"
        id="inputRole"
        placeholder="Role"
        required
      />
      <label for="graduationYear">Graduation Year</label>
      <input
        type="number"
        name="graduationYear"
        id="inputGraduationYear"
        placeholder="Graduation Year"
        required
      />
      <label for="major">Major</label>
      <input
        type="text"
        name="major"
        id="inputMajor"
        placeholder="Major"
        required
      />
      <label for="job">Job</label>
      <input
        type="text"
        name="job"
        id="inputJob"
        placeholder="Job"
      />
      <label for="company">Company</label>
      <input
        type="text"
        name="company"
        id="inputCompany"
        placeholder="Company"
      />
      <label for="city">City</label>
      <input
        type="text"
        name="city"
        id="inputCity"
        placeholder="City"
      />
      <label for="state">State</label>
      <input
        type="text"
        name="state"
        id="inputState"
        placeholder="State"
      />
      <label for="country">Country</label>
      <input
        type="text"
        name="country"
        id="inputCountry"
        placeholder="Country"
      />
      <label for="bio">Bio</label>
      <input
        type="text"
        name="bio"
        id="inputBio"
        placeholder="Bio"
      />
      <label for="interests">Interests</label>
      <input
        type="text"
        name="interests"
        id="inputInterest"
        placeholder="Interests"
      />
      
      
      <button type="submit">Sign in</button>
    </form>
  </div>
  